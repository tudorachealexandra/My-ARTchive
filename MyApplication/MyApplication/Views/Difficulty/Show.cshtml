@model MyApplication.Models.Difficulty
@{
    ViewBag.Title = "Show difficulty level";
}

<h2>@ViewBag.Title</h2>

<p>@Model.DifficultyName</p>

<div class="products">
    <div class="row">
        <div class="container">
            <div class="row">
                @foreach (MyApplication.Models.Job job in ViewBag.Jobs)
                {
                        @Html.Partial("JobInfo", job)
                }
            </div>
        </div>
    </div>
</div>
<br />
<a class="btn btn-success" href="/Difficulty/Index/">Show list of difficulty levels</a>

<br />
<hr />
